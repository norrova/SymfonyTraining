{% extends 'base.html.twig' %}
{% block body %}
    <h1>Pins</h1>
    {% if pins is defined %}
        {% for pin in pins %}
            {{ dump(pin) }}
            <!-- Objet pin accÃ©der par "obj.propertie"-->
            <article>
                <!-- call getter => getTitle-->
                <h1>{{ pin.title }}</h1>
                <!-- call getter => getDescription-->
                <p>{{ pin.description }}</p>
            </article>
        {% endfor %}
    {% endif %}
{% endblock %}